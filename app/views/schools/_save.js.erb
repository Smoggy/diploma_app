    $("ul.errors").html("")
    <% if @school.errors.any? %>
    <% @school.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
    <% end %>
    <% else %>
    $(".school-index").html("<%= escape_javascript(render 'index') %>")
    $("#school-modal").modal("hide")
    <% end %>